<project name="seedy-maven-plugin">
    <bannerLeft>
        <name>seedy-maven-plugin</name>
        <href>https://github.com/bitblit/seedy-maven-plugin</href>
        <src>header.png</src>
    </bannerLeft>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <!-- http://maven.apache.org/skins/maven-fluido-skin/ -->
    <!-- from example https://github.com/apache/maven-surefire/blob/master/src/site/site.xml
     see it in action http://maven.apache.org/surefire/maven-failsafe-plugin/ -->
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <googleSearch>
                <sitesearch>${project.url}</sitesearch>
            </googleSearch>
            <gitHub>
                <projectId>bitblit/seedy-maven-plugin</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <menu ref="reports" inherit="top"/>
        <menu ref="modules"/>
        <menu name="Usage">
            <item href="release-notes.html" name="Release Notes" />
            <item href="general-usage.html" name="General Usage" />
            <item href="s3-upload-usage.html" name="Usage: S3Upload" />
            <item href="deploy-lambda-api-usage.html" name="Usage: Deploy Lambda API" />
            <item href="start-new-environment-usage.html" name="Usage: Start New Environment" />
        </menu>
        <menu name="Other Notes">
            <item href="http://wickedsource.org/2013/09/23/releasing-your-project-to-maven-central-guide/" name="Doing Maven releases" />
            <item href="https://help.github.com/articles/creating-project-pages-manually/" name="Publishing pages on Github" />
        </menu>
    </body>
</project>
